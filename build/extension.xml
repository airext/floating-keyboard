<extension xmlns="http://ns.adobe.com/air/extension/32.0">
  <id>com.github.airext.NativeKeyboardText</id>
  <versionNumber>0.20.0</versionNumber>
  <platforms>
    <platform name="iPhone-ARM">
      <applicationDeployment>
        <nativeLibrary>libANXNativeKeyboardText.a</nativeLibrary>
        <initializer>ANXNativeKeyboardTextInitializer</initializer>
        <finalizer>ANXNativeKeyboardTextFinalizer</finalizer>
      </applicationDeployment>
    </platform>
    <platform name="iPhone-x86">
      <applicationDeployment>
        <nativeLibrary>libANXNativeKeyboardTextSimulator.a</nativeLibrary>
        <initializer>ANXNativeKeyboardTextInitializer</initializer>
        <finalizer>ANXNativeKeyboardTextFinalizer</finalizer>
      </applicationDeployment>
    </platform>
    <platform name="default">
      <applicationDeployment/>
    </platform>
  </platforms>
</extension>
